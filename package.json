  },
  "homepage": "https://github.com/mikerovers/file-orderer#readme",
  "dependencies": {
    "aws-sdk": "^2.82.0",
    "electron": "^1.6.11",
    "electron-packager": "^8.7.2",
    "electron-reload": "^1.2.1",
    "extfs": "0.0.7",
    "fs-extra": "^4.0.0",
    "laravel-mix": "^1.1.1",
    "net": "^1.0.2",
    "node-gyp": "^3.6.2",
    "react-file-input": "^0.2.5",
    "react-sortable-hoc": "^0.6.5",
    "tls": "0.0.1",
    "webpack": "^3.1.0",
    "write-json-file": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bootstrap": "4.0.0-alpha.6",
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "child_process": "^1.0.2",
    "electron-react-devtools": "^0.4.0",
    "fs": "0.0.1-security",
